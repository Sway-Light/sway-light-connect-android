<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="8dp"
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp">
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="@android:color/white" />
        </shape>
    </item>
    <item
        android:top="8dp"
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp">
        <rotate
            android:fromDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="-90">
            <shape
                android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="@android:color/white" />
            </shape>
        </rotate>
    </item>

    <item>
        <shape android:shape="oval">
            <size android:width="30dp"
                android:height="30dp"/>
            <stroke android:width="1dp"
                android:color="@android:color/white"/>
        </shape>
    </item>
</layer-list>